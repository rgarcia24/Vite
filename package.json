{
  "name": "vite",
  "version": "1.0.0",
  "description": "Its a bot",
  "main": "./src/main/index.js",
  "scripts": {
    "start": "concurrently \"yarn run webpack-dev && yarn run react-dev\"  \"wait-on http://localhost:3000 && yarn run electron-dev\"",
    "webpack-dev": "webpack --mode development",
    "react-dev": "webpack-dev-server --mode development --port 3000",
    "electron-dev": "cross-env ELECTRON_ENV=dev electron --trace-warnings ./build/app.js ",
    "webpack-prod": "webpack --mode production",
    "build-win": "electron-builder build --win -c.extraMetadata.main=./build/app.js --publish never",
    "build-mac": "electron-builder build --mac -c.extraMetadata.main=./build/app.js --publish never",
    "build-linux": "electron-builder build --linux -c.extraMetadata.main=./build/app.js --publish never",
    "clean": "rm -rf ./build ./dist",
    "fullClean": "rm -rf ./build ./dist ./node_modules yarn-error.log"
  },
  "author": "Ven",
  "license": "MIT",
  "build": {
    "appId": "com.vite",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "productName": "Vite",
    "win": {
      "asar": true,
      "target": "nsis",
      "icon": "vite.ico"
    },
    "mac": {
      "asar": true,
      "icon": "vite.icns",
      "target": "dmg"
    }
  },
  "dependencies": {
    "@mantine/core": "^4.0.7",
    "@mantine/dates": "^4.0.7",
    "@mantine/dropzone": "^4.0.7",
    "@mantine/form": "^4.0.7",
    "@mantine/hooks": "^4.0.7",
    "@mantine/modals": "^4.0.7",
    "@mantine/notifications": "^4.0.7",
    "@mantine/prism": "^4.0.7",
    "@mantine/rte": "^4.0.7",
    "@mantine/spotlight": "^4.0.7",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.4",
    "axios": "^0.21.1",
    "capmonster": "^0.0.3",
    "chalk": "^4.1.2",
    "chart.js": "^3.3.2",
    "classnames": "^2.3.1",
    "concurrently": "^6.5.1",
    "credit-card-type": "^9.1.0",
    "dayjs": "^1.11.0",
    "discord-rich-presence": "0.0.8",
    "electron-store": "^8.0.1",
    "framer-motion": "4.1.17",
    "got": "^11.8.2",
    "howler": "^2.2.3",
    "node-adyen-encrypt": "^2.3.1",
    "react": "^16.13.1",
    "react-chartjs-2": "^3.0.3",
    "react-credit-cards": "^0.8.3",
    "react-dom": "^16.13.1",
    "react-number-format": "^4.9.1",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^7.0.4",
    "react-windowed-select": "^3.1.2",
    "regenerator-runtime": "^0.13.9",
    "socket.io": "^4.3.1",
    "sound-play": "^1.1.0",
    "tabler-icons-react": "^1.47.0",
    "tough-cookie": "^4.0.0",
    "tunnel": "^0.0.6",
    "uuid": "^8.3.2",
    "uuid4": "^2.0.2",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "javascript-obfuscator": "^4.0.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-electron-reload": "^1.0.1",
    "webpack-obfuscator": "2.6.0"
  }
}